[ignore]
.*/build/.*
.*/public/.*
.*/static/.*
.*/packages/.*/lib
.*/packages/.*/test

[include]

[libs]

[lints]

[options]
module.name_mapper='^@src\/\(.*\)$' -> '<PROJECT_ROOT>/src/\1'
module.name_mapper='^@spec\/\(.*\)$' -> '<PROJECT_ROOT>/src/specification/\1'
module.name_mapper='^@state\/\(.*\)$' -> '<PROJECT_ROOT>/src/state/\1'
module.name_mapper='^@pages\/\(.*\)$' -> '<PROJECT_ROOT>/src/pages/\1'
module.name_mapper='^@images\/\(.*\)$' -> '<PROJECT_ROOT>/src/images/\1'
module.name_mapper='^@components\/\(.*\)$' -> '<PROJECT_ROOT>/src/components/\1'

[strict]
